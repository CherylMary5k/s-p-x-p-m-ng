template < class T>
	void nhap(T a[], int n)
	{
		int i;
		cout << "Nhap cac phan tu cua mang :\n";
		for (i = 0; i < n; i++)
		{
			cout << "A[" << i + 1 << "]=";
			cin >> a[i];
		}
	}

